{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Robots-Tag", "value": "noindex" },
        { "key": "Cache-Control", "value": "public, max-age=0, must-revalidate" }
      ]
    }
  ],
  "github": { "enabled": false },
  "git": { "forkProtection": false },
  "security": {
    "botProtection": false
  }
}
